[
  // デフォルトだとスタックの中から次/前タブに移るので
  { "keys": ["ctrl+tab"], "command": "next_view" },
  { "keys": ["ctrl+shift+tab"], "command": "prev_view" },
  { "keys": ["ctrl+shift+p"], "command": "prompt_select_project" },
  { "keys": ["ctrl+_"], "command": "undo" },
  { "keys": ["alt+_"], "command": "redo" },
  // 矩形選択
  { "keys": ["alt+p"], "command": "select_lines", "args": {"forward": false} },
  { "keys": ["alt+n"], "command": "select_lines", "args": {"forward": true} },
  { "keys": ["ctrl+c"], "command": "exit_insert_mode" },
    { "keys": ["ctrl+shift+c"], "command": "copy" }
]
